{
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.10",
    "express": "^5.1.0",
    "prisma": "^6.11.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.11.0",
    "cors": "^2.8.5",
    "serverless-http": "^3.2.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "vercel-build": "npx prisma generate && npx prisma migrate deploy"
  },
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.ts"
    }
  ]
}
